.container{
  margin: 0rpx;
  padding: 0rpx;
}
.page {
  background-color:#F9F9F9;
}
.studyBox {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap:wrap;
  width: 100%;
  height: 100%;
  margin-top: 3rpx;
}

.usermott {
  font-size: 25rpx;
  width: 19%;
  height: 160rpx;
}
.logo {
  width: 35%;
  height: 35%;
  margin-top: 15%;
  margin-left: 32.5%;
}
.txt {
  width: 100%;
  height: 40rpx;
  text-align: center;
  margin-bottom: 0;
  font-size: 30rpx;
}
.cultivate{
  width: 100%;
  height: 2200rpx;
  display: flex;
  flex-direction: column;
  background-color: rgb(238, 238, 238);
}
.backColor{  
  background-color: white;
  margin-top: 30rpx;
}
.contentTitle{
  height: 110rpx;
