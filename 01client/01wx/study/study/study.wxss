  flex-direction: row;
  align-items: center;
  margin-top: 20rpx;
}
.redLine{
  width: 10rpx;
  height: 60rpx;
  background-color: red;
  margin-left: 12rpx;
}
.cultivateT{
  width:300rpx;
  margin-left: 15rpx;
  font-size: 40rpx;
}
.more{
  margin-left: 40%;
  font-size: 33rpx;
  color: grey;
}
.iconright{
  width: 30rpx;
  height: 30rpx;
}
.ContentL{
  margin: 3% 1%;
  padding: 3%; 
  border-bottom: 2rpx solid rgb(224, 224, 224);
}
.Content{
  margin: 0% 4%;
}
.font{
  height: 50rpx;
  width: 100%;
